<?xml version="1.0" encoding="utf-8"?>

<shape

    xmlns:android="http://schemas.android.com/apk/res/android"

    android:shape="rectangle">

    <!-- 填充的颜色 -->

    <solid android:color="@color/bar_grey_90"/>

    <!-- 设置按钮的四个角为弧形 -->

    <!-- android:radius 弧形的半径 -->

    <corners
        android:topLeftRadius="75dip"
        android:bottomLeftRadius="75dip" />

    <stroke
        android:width="1dp"
        android:color="@color/bar_grey_90" />


</shape>
